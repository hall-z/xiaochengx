/* vendor/nabi/_wechat/pages/campaign/index.wxss */

.campaign-container {
  position: relative;
}

.campaign-scrollbox {
  position: relative;
  width: 100%;
  height: 100vh;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.campaign-unscroll {
  width: 100%;
  height: 100vh;
  overflow: hidden;
}

.campaign-hide {
  display: none;
}

@import "../../templates/modal.wxss";
